{
  "name": "nine1bot",
  "version": "1.0.0",
  "description": "AI coding assistant with web interface and tunnel support",
  "type": "module",
  "bin": {
    "nine1bot": "./bin/nine1bot"
  },
  "main": "./src/index.ts",
  "scripts": {
    "dev": "bun run --conditions=browser ./src/index.ts",
    "build": "bun build ./src/index.ts --outdir=./dist --target=node",
    "start": "bun run ./src/index.ts"
  },
  "dependencies": {
    "@clack/prompts": "^0.9.1",
    "@ngrok/ngrok": "^1.4.1",
    "yargs": "^17.7.2",
    "zod": "^3.23.8",
    "open": "^10.1.0"
  },
  "devDependencies": {
    "@types/node": "^22.10.2",
    "@types/yargs": "^17.0.33",
    "typescript": "^5.8.2"
  },
  "peerDependencies": {
    "bun": ">=1.0.0"
  },
  "engines": {
    "node": ">=18.0.0"
  },
  "keywords": [
    "ai",
    "coding",
    "assistant",
    "cli",
    "tunnel",
    "ngrok"
  ],
  "author": "",
